{
    "manifest_version": 3,
    "name": "GCP Selector",
    "version": "0.2",
    "description": "Jump to GCP console at once",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "options_page": "option/option.html",
    "permissions": [
        "storage"
    ]
}
